{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "key": "Access-Control-Allow-Credentials",
      "value": "true"
    },
    {
      "key": "Access-Control-Allow-Origin",
      "value": "https://digitalalumniconnecter.vercel.app"
    }
  ]
}